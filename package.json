{
  "name": "kavak-ai-agent",
  "version": "1.0.0",
  "description": "Agente comercial AI de Kavak basado en serverless",
  "main": "index.js",
  "scripts": {
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:dev:env": "serverless deploy --stage dev --verbose",
    "deploy:prod:env": "serverless deploy --stage prod --verbose",
    "test": "pytest",
    "lint": "flake8 . && black . --check",
    "format": "black .",
    "offline": "serverless offline"
  },
  "dependencies": {
    "serverless-offline": "^13.3.3",
    "serverless-python-requirements": "^6.0.0"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-step-functions": "^3.23.0"
  },
  "author": "Kavak AI Team",
  "license": "UNLICENSED",
  "private": true
}
